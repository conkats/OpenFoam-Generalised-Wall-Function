FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (30.222315557 7.4625 0);

boundaryField
{
    farfield
    {
        type            freestream;
        freestreamValue uniform (30.222315557 7.4625 0); //13.87 degree angle
    }


    airfoil
    {
        //type           noSlip;
	    type		fixedValue;
        value		uniform (0 0 0);
    }

    frontAndBack
    {
        type            empty;
    }
}

