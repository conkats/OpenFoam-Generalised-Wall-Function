FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;//0.001;

boundaryField
{
    farfield
    {
        type            freestream;
        freestreamValue uniform 0.001;
    }

    airfoil
    {
        type            nutkGWF;
        value           $internalField;
    }

    frontAndBack
    {
        type            empty;
    }
}

